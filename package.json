    "react-dom": "^15.1.0",
    "react-modal": "^1.4.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.1",
    "redux": "^3.6.0",
    "redux-form": "^5.3.2",
    "redux-thunk": "^2.1.0",
    "uuid": "^2.0.3",
    "whatwg-fetch": "^2.0.3"
  }
