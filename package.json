{
  "name": "isomorphic-portfolio",
  "version": "1.0.0",
  "description": "An isomorphic portfolio website to showcase my projects.",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/oandrienko/isomorphic-portfolio"
  },
  "author": "Oles Andrienko",
  "license": "ISC",
  "engines": {
    "node": "6.2.2"
  },
  "scripts": {
    "server": "node ./build/server.js",
    "devServer": "nodemon --exec babel-node ./src/server.js",
    "buildServer": "babel ./src --out-dir ./build",
    "build": "node bin/build",
    "dev": "node bin/start",
    "start": "npm run server",
    "build:prod": "npm run build -- -p",
    "build:prod:watch": "npm run build -- -p -w",
    "build:watch": "npm run build -- -w",
    "build:watch:prod": "npm run build:prod:watch"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^8.2.1",
